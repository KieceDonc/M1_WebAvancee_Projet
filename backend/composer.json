{
    "name": "m1/e-commerce",
    "version": "0.0.0",
    "type": "project",
    "description": "Site de e-commerce Projet M1",
    "keywords": ["m1", "e-commerce"],
    "require": {
        "php": "^8.0.2",
        "doctrine/migrations": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "webpatser/laravel-uuid": "^4.0",
        "doctrine/dbal": "3.3",
        "cweagans/composer-patches": "^1.7",
        "symplify/vendor-patches": "^11.0",
        "cyclonedx/cyclonedx-php-composer": "^3.10"
    },
    "require-dev": {
        "based/laravel-typescript": "^0.0.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "reliese/laravel": "^1.1",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "patches": {
            "laravel/framework": [
                "patches/laravel-framework-src-illuminate-database-migrations-migrator-php.patch"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "cyclonedx/cyclonedx-php-composer": true,
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
